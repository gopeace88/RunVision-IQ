<?xml version="1.0"?>
<!-- RunVision-IQ - Garmin Connect IQ DataField -->
<!-- iLens AR 글래스 연동 러닝/사이클링 메트릭 디스플레이 -->
<iq:manifest xmlns:iq="http://www.garmin.com/xml/connectiq" version="3">
    <iq:application
        entry="RunVisionIQApp"
        id="c4e8f9a3-1b2d-4c5e-9f8a-7b6d5e4a3c2b"
        launcherIcon="@Drawables.LauncherIcon"
        minApiLevel="3.1.0"
        name="@Strings.AppName"
        type="datafield"
        version="1.0.0">

        <iq:products>
            <!-- Primary Target: Forerunner 265 -->
            <iq:product id="fr265"/>
            <iq:product id="fr265s"/>

            <!-- Venu Series (BLE Central 지원) -->
            <iq:product id="venusq"/>
            <iq:product id="venusq2"/>

            <!-- Phase 1: Forerunner Series (BLE Central 지원) -->
            <iq:product id="fr955"/>
            <iq:product id="fr965"/>

            <!-- Phase 2: Fenix 7 Series (BLE Central 지원) -->
            <iq:product id="fenix7"/>
            <iq:product id="fenix7s"/>
            <iq:product id="fenix7x"/>
            <iq:product id="fenix7pro"/>
            <iq:product id="fenix7spro"/>
            <iq:product id="fenix7xpro"/>
        </iq:products>

        <iq:permissions>
            <!-- BLE Central for iLens connection -->
            <iq:uses-permission id="BluetoothLowEnergy"/>
            <!-- User weight/age for power calculation -->
            <iq:uses-permission id="UserProfile"/>
        </iq:permissions>

        <iq:languages>
            <iq:language>eng</iq:language>
            <iq:language>kor</iq:language>
        </iq:languages>
    </iq:application>
</iq:manifest>
